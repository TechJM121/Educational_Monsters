# Staging Environment Configuration
# DO NOT commit this file to version control

# Supabase Configuration
VITE_SUPABASE_URL=https://your-staging-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-staging-anon-key

# Monitoring and Analytics
VITE_MONITORING_ENDPOINT=https://staging-monitoring.educational-rpg-tutor.com/api
VITE_ANALYTICS_ID=your-staging-analytics-id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_DEBUG_MODE=true

# CDN Configuration
VITE_CDN_URL=https://staging-cdn.educational-rpg-tutor.com

# Security Settings
VITE_ENABLE_CSP=true
VITE_ENABLE_RATE_LIMITING=false

# Application Settings
VITE_APP_VERSION=1.0.0-staging
VITE_APP_ENVIRONMENT=staging